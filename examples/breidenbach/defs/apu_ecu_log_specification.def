events{

    APU_ECU_CommunicationChanged{
 description{"CAN 4: APU ECU CommunicationStatus changed.";};
 };
 
    APU_ECU_InternalErrorChanged{
 description{"APU ECU Internal Error State changed.";};
 };
 

};

state{
 id{APU_ECU_Communication;};
 value{1 * offline;};
};


state{
 id{APU_ECU_InternalError;};
 value{-1;}; /* id */
 value{-1;}; /* state */
 //value{-1;}; /* count */
};

record{
 id{rec_apu_ecu_status;};
  APU_ECU_Communication;
  APU_ECU_InternalError;
};