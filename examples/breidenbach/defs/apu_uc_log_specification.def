Description:
	APU UC specific events and states
*/ 


events{


 
    APU_UC_CommunicationChanged{
 description{"CAN 4: APU UC CommunicationStatus changed.";};
 };
 
     APU_UC_InternalErrorChanged{
 description{"APU UC Internal Error State changed.";};
 };
 
     APU_UC_MonitoringStatusChanged{
 description{"APU UC MonitoringStatus changed.";};
 };
 
};

state{
 id{APU_UC_Communication;};
 value{1 * offline;};
};

state{
 id{APU_UC_InternalError;};
 value{-1;}; /* id */
 value{-1;}; /* state */
 value{-1;}; /* count */
};


state{
 id{APU_UC_R_Temperature;};
 value{0;};
};

state{
 id{APU_UC_Temperature;};
 value{0;};
};

state{
 id{APU_UC_PCB_Temperature;};
 value{1;};
};

state{
 id{APU_UC_BoostStatus;};
 value{1;};
};

state{
 id{APU_UC_LoadStatus;};
 value{1;};
};

state{
 id{APU_UC_UnloadStatus;};
 value{1;};
};

state{
 id{APU_UC_MonitoringStatus;};
 value{1;};
};

record{
 id{rec_apu_uc_status;};
  APU_UC_Communication;
  APU_UC_InternalError;  
  APU_UC_Temperature;
  APU_UC_R_Temperature;
  APU_UC_PCB_Temperature;
  APU_UC_BoostStatus;
  APU_UC_LoadStatus;
  APU_UC_UnloadStatus;
  APU_UC_MonitoringStatus;  
};