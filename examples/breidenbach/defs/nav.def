events{

   NAV_CAN_CommunicationChanged{
 description{"CAN2 / SerialBus: NAV CAN CommunicationStatus changed.";};
 };
 
    NAV_StatusChanged{
 description{"NAV Status changed.";};
 };
 
};

state{
 id{NAV_CommunicationStatus;};
 value{0;};
};

state{
 id{NAV_gpsFail;};
 value{0;};
};

state{
 id{NAV_imuFail;};
 value{0;};
};

state{
 id{NAV_vmsFail;};
 value{0;};
};

state{
 id{NAV_fault;};
 value{0;};
};

state{
 id{NAV_MapDatum;};
 value{0;};
};

state{
 id{NAV_GPS_Type;};
 value{0;};
};

state{
 id{NAV_GPS_Power;};
 value{0;};
};


state{
 id{NAV_GPS_AntennaInternal;};
 value{0;};
};

record{
 id{rec_nav_status;};
  NAV_CommunicationStatus;
  NAV_gpsFail;  
  NAV_imuFail;
  NAV_vmsFail;
  NAV_fault;
  NAV_MapDatum;
  NAV_GPS_Type;
  NAV_GPS_Power;
  NAV_GPS_AntennaInternal;
};