events{

    APU_ECU_InternalErrorChanged{
 description{"APU UC MonitoringStatus changed.";};
 };
 
    APU_ECU_CommunicationChanged{
 description{"CAN 4: APU UC CommunicationStatus changed.";};
 };
 
};

state{
 id{APU_ECU_Communication;};
 value{1 * offline;};
};


state{
 id{APU_ECU_InternalError;};
 value{0;};
};


record{
 id{apu_ecu_status;};
  APU_ECU_Communication;
  APU_ECU_InternalError;
  
};